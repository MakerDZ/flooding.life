generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model Hero {
  id          String   @id @default(auto()) @map("_id") @db.ObjectId
  name        String
  pfp         String
  wish        String
  twitterLink String
  createdAt   DateTime @default(now())
}
